<h1>Stocks!</h1>


<h1>List of Stocks</h1>
 
<table>
  <tr>
    <th>Symbol</th>
    <th>Target</th>
    <th></th>
  </tr>
 
  <% @stocks.each do |stock| %>
    <tr>
      <td><%= stock.symbol %></td>
      <td><%= stock.target %></td>
      <td><%= link_to 'Show', stock_path(stock) %></td>
    </tr>
  <% end %>
</table>